@model ExtractViewModel
@{
    ViewData["Title"] = "Index";
}

<form method="post" enctype="multipart/form-data" asp-controller="Extract" asp-action="Create">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload de um ou mais arquivos Ofx</p>
            <input type="file" name="files" multiple=""/>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Importar"/>
        </div>
    </div>
</form>
